{
  "name": "@joonhocho/typescript-esmodule-library-template",
  "version": "0.0.1",
  "description": "TypeScript ECMAScript Module Library Template",
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "pub": "node publish.js",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write {src,test}/**/*.{ts,tsx,js,jsx}",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/typescript-esmodule-library-template.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/typescript-esmodule-library-template/issues"
  },
  "homepage": "https://github.com/joonhocho/typescript-esmodule-library-template#readme",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.4.1",
    "jest": "^29.5.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "prettier": "^2.8.4",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
